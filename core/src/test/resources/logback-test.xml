<?xml version="1.0" encoding="UTF-8"?>

<configuration>

	<appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{HH:mm:ss.SSS} %-5level {%thread} (%F:%L\) [%logger{20}] : %msg%n</pattern>
		</encoder>
	</appender>

	<logger name="org.springframework.ws.ext" level="TRACE" />

	<root level="INFO">
		<appender-ref ref="CONSOLE" />
	</root>

</configuration>
