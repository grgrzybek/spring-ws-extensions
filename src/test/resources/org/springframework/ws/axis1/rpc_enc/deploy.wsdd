<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from EchoEndpointRPCService WSDL service -->

  <service name="EchoEndpointRPCEncoded" provider="java:RPC" style="rpc" use="encoded">
      <parameter name="wsdlTargetNamespace" value="http://dispatch2.axis1.ws.springframework.org/"/>
      <parameter name="wsdlServiceElement" value="EchoEndpointRPCService"/>
      <parameter name="schemaUnqualified" value="http://dispatch2.axis1.ws.springframework.org/p2/,http://dispatch2.axis1.ws.springframework.org/p3/,http://dispatch2.axis1.ws.springframework.org/p1/,http://dispatch2.axis1.ws.springframework.org/r1/,http://dispatch2.axis1.ws.springframework.org/"/>
      <parameter name="wsdlServicePort" value="EchoEndpointRPCEncoded"/>
      <parameter name="className" value="org.springframework.ws.axis1.rpc_enc.EchoEndpointRPCEncodedSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="EchoEndpointRPC"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="rpcEnc" qname="operNS:rpcEnc" xmlns:operNS="http://dispatch2.axis1.ws.springframework.org/" returnQName="rpcEncReturn" returnType="rtns:result-1" xmlns:rtns="http://dispatch2.axis1.ws.springframework.org/r1/" soapAction="" >
        <parameter qname="param1" type="tns:param-1" xmlns:tns="http://dispatch2.axis1.ws.springframework.org/p1/"/>
        <parameter qname="param2" type="tns:param-2" xmlns:tns="http://dispatch2.axis1.ws.springframework.org/p2/"/>
      </operation>
      <parameter name="allowedMethods" value="rpcEnc"/>

      <typeMapping
        xmlns:ns="http://dispatch2.axis1.ws.springframework.org/r1/"
        qname="ns:result-1"
        type="java:org.springframework.ws.axis1.rpc_enc.Result1"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
      />
      <arrayMapping
        xmlns:ns="http://dispatch2.axis1.ws.springframework.org/"
        qname="ns:ArrayOf_soapenc_decimal"
        type="java:java.math.BigDecimal[]"
        innerType="cmp-ns:decimal" xmlns:cmp-ns="http://schemas.xmlsoap.org/soap/encoding/"
        encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
      />
      <typeMapping
        xmlns:ns="http://dispatch2.axis1.ws.springframework.org/p2/"
        qname="ns:param-2"
        type="java:org.springframework.ws.axis1.rpc_enc.Param2"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
      />
      <arrayMapping
        xmlns:ns="http://dispatch2.axis1.ws.springframework.org/"
        qname="ns:ArrayOf_soapenc_string"
        type="java:java.lang.String[]"
        innerType="cmp-ns:string" xmlns:cmp-ns="http://schemas.xmlsoap.org/soap/encoding/"
        encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
      />
      <typeMapping
        xmlns:ns="http://dispatch2.axis1.ws.springframework.org/p3/"
        qname="ns:param-3"
        type="java:org.springframework.ws.axis1.rpc_enc.Param3"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
      />
      <typeMapping
        xmlns:ns="http://dispatch2.axis1.ws.springframework.org/p1/"
        qname="ns:param-1"
        type="java:org.springframework.ws.axis1.rpc_enc.Param1"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
      />
      <arrayMapping
        xmlns:ns="http://dispatch2.axis1.ws.springframework.org/"
        qname="ns:ArrayOf_soapenc_byte"
        type="java:java.lang.Byte[]"
        innerType="cmp-ns:byte" xmlns:cmp-ns="http://schemas.xmlsoap.org/soap/encoding/"
        encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
      />
      <arrayMapping
        xmlns:ns="http://dispatch2.axis1.ws.springframework.org/"
        qname="ns:ArrayOf_tns3_param-3"
        type="java:org.springframework.ws.axis1.rpc_enc.Param3[]"
        innerType="cmp-ns:param-3" xmlns:cmp-ns="http://dispatch2.axis1.ws.springframework.org/p3/"
        encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
      />
  </service>
</deployment>
