<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from EchoEndpointDocumentService WSDL service -->

  <service name="EchoEndpointDocumentLiteral" provider="java:RPC" style="document" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://dispatch2.axis1.ws.springframework.org/"/>
      <parameter name="wsdlServiceElement" value="EchoEndpointDocumentService"/>
      <parameter name="schemaQualified" value="http://dispatch2.axis1.ws.springframework.org/p2/,http://dispatch2.axis1.ws.springframework.org/p3/,http://dispatch2.axis1.ws.springframework.org/p1/,http://dispatch2.axis1.ws.springframework.org/r1/,http://dispatch2.axis1.ws.springframework.org/"/>
      <parameter name="wsdlServicePort" value="EchoEndpointDocumentLiteral"/>
      <parameter name="className" value="org.springframework.ws.axis1.doc_lit.EchoEndpointDocumentLiteralSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="EchoEndpointDocument"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="docLit" qname="docLit" returnQName="retNS:docLitReturn" xmlns:retNS="http://dispatch2.axis1.ws.springframework.org/" returnType="rtns:result-1" xmlns:rtns="http://dispatch2.axis1.ws.springframework.org/r1/" soapAction="" >
        <parameter qname="pns:param" xmlns:pns="http://dispatch2.axis1.ws.springframework.org/" type="tns:param-1" xmlns:tns="http://dispatch2.axis1.ws.springframework.org/p1/"/>
        <parameter qname="pns:param2" xmlns:pns="http://dispatch2.axis1.ws.springframework.org/" type="tns:param-2" xmlns:tns="http://dispatch2.axis1.ws.springframework.org/p2/"/>
      </operation>
      <parameter name="allowedMethods" value="docLit"/>

      <arrayMapping
        xmlns:ns="http://dispatch2.axis1.ws.springframework.org/"
        qname="ns:ArrayOf_xsd_decimal"
        type="java:java.math.BigDecimal[]"
        innerType="cmp-ns:decimal" xmlns:cmp-ns="http://www.w3.org/2001/XMLSchema"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://dispatch2.axis1.ws.springframework.org/r1/"
        qname="ns:result-1"
        type="java:org.springframework.ws.axis1.doc_lit.Result1"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <arrayMapping
        xmlns:ns="http://dispatch2.axis1.ws.springframework.org/"
        qname="ns:ArrayOf_xsd_string"
        type="java:java.lang.String[]"
        innerType="cmp-ns:string" xmlns:cmp-ns="http://www.w3.org/2001/XMLSchema"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://dispatch2.axis1.ws.springframework.org/p2/"
        qname="ns:param-2"
        type="java:org.springframework.ws.axis1.doc_lit.Param2"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://dispatch2.axis1.ws.springframework.org/p3/"
        qname="ns:param-3"
        type="java:org.springframework.ws.axis1.doc_lit.Param3"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <arrayMapping
        xmlns:ns="http://dispatch2.axis1.ws.springframework.org/"
        qname="ns:ArrayOf_xsd_byte"
        type="java:byte[]"
        innerType="cmp-ns:byte" xmlns:cmp-ns="http://www.w3.org/2001/XMLSchema"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://dispatch2.axis1.ws.springframework.org/p1/"
        qname="ns:param-1"
        type="java:org.springframework.ws.axis1.doc_lit.Param1"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <arrayMapping
        xmlns:ns="http://dispatch2.axis1.ws.springframework.org/"
        qname="ns:ArrayOf_tns3_param-3"
        type="java:org.springframework.ws.axis1.doc_lit.Param3[]"
        innerType="cmp-ns:param-3" xmlns:cmp-ns="http://dispatch2.axis1.ws.springframework.org/p3/"
        encodingStyle=""
      />
  </service>
</deployment>
