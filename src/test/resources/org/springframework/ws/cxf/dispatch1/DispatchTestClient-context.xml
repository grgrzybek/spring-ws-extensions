<?xml version="1.0" encoding="UTF-8"?>

<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:jaxws="http://cxf.apache.org/jaxws"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd">

	<jaxws:client id="clientDocument" serviceClass="org.springframework.ws.cxf.dispatch1.EchoEndpointDocumentInterface" address="http://localhost/cxf/echoDocument" />
	<jaxws:client id="clientRPC" serviceClass="org.springframework.ws.cxf.dispatch1.EchoEndpointRPCInterface" address="http://localhost/cxf/echoRPC" />

</beans>
